{
    "name": "@yicode/yiadmin",
    "version": "1.3.13",
    "description": "前后分离的 vue3 + vite + yite-cli 后台管理模板",
    "sideEffects": true,
    "type": "module",
    "private": false,
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org"
    },
    "scripts": {
        "dev": "yite --command=dev --envfile=development",
        "build": "yite --command=build --envfile=production",
        "update:template": "yite --command=update --project-type=yiadmin"
    },
    "keywords": [
        "lodash",
        "utils",
        "helper",
        "help"
    ],
    "author": "chensuiyi <bimostyle@qq.com>",
    "repository": {
        "type": "git",
        "url": "https://github.com/chenbimo/yicode.git"
    },
    "homepage": "https://yicode.tech",
    "dependencies": {
        "@arco-design/web-vue": "^2.55.1",
        "@arco-plugins/vite-vue": "^1.4.5",
        "@iconify/json": "^2.2.204",
        "@yicode/yidash": "workspace:*",
        "axios": "^1.6.8",
        "date-fns": "^3.6.0",
        "js-md5": "^0.8.3",
        "lodash-es": "^4.17.21",
        "pinia": "^2.1.7",
        "sass": "^1.75.0",
        "store2": "^2.14.3",
        "unocss": "^0.59.4",
        "vue": "^3.4.25",
        "vue-i18n": "^9.13.1",
        "vue-router": "^4.3.2"
    },
    "devDependencies": {
        "@yicode/yite-cli": "workspace:*"
    }
