{
    "name": "@yicode/yicode-cli",
    "version": "0.0.8",
    "description": "yicode（易编程） - 命令行实用工具",
    "author": "chensuiyi <bimostyle@qq.com>",
    "bin": {
        "mynewcli": "./bin/run"
    },
    "homepage": "https://yicode.tech",
    "license": "GPL-3.0",
    "main": "dist/index.js",
    "repository": "https://github.com/chenbimo/yicode.git",
    "files": [],
    "dependencies": {
        "@oclif/core": "^2.8.5",
        "@oclif/plugin-help": "^5.2.9",
        "@oclif/plugin-plugins": "^2.4.7"
    },
    "devDependencies": {
        "oclif": "^3.9.0",
        "shx": "^0.3.4"
    },
    "oclif": {
        "bin": "mynewcli",
        "dirname": "mynewcli",
        "commands": "./dist/commands",
        "plugins": [
            "@oclif/plugin-help",
            "@oclif/plugin-plugins"
        ],
        "topicSeparator": " ",
        "topics": {
            "hello": {
                "description": "Say hello to the world and others"
            }
        }
    },
    "scripts": {
        "build": "shx rm -rf dist",
        "postpack": "shx rm -f oclif.manifest.json",
        "prepack": "npm run build && oclif manifest && oclif readme",
        "version": "oclif readme && git add README.md"
    },
    "engines": {
        "node": ">=12.0.0"
    },
    "keywords": [
        "oclif"
    ]
}
