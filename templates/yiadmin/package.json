        "date-fns": "^3.3.1",
        "js-md5": "^0.8.3",
        "lodash-es": "^4.17.21",
        "pinia": "^2.1.7",
        "sass": "^1.71.0",
        "store2": "^2.14.3",
        "vue": "^3.4.19",
        "vue-i18n": "^9.9.1",
        "vue-router": "^4.2.5"
    },
    "devDependencies": {
