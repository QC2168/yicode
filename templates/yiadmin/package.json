        "@yicode/yidash": "3.6.1",
        "axios": "^1.5.1",
        "date-fns": "^2.30.0",
        "lodash-es": "^4.17.21",
        "pinia": "^2.1.7",
        "sass": "^1.69.4",
        "store2": "^2.14.2",
        "vue": "^3.3.4",
        "vue-i18n": "^9.5.0",
        "vue-router": "^4.2.5"
    },
    "devDependencies": {
