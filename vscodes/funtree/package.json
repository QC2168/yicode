{
    "name": "@yicode-vscode/funtree",
    "displayName": "vscode 侧边栏函数列表插件",
    "description": "function tree",
    "publisher": "chensuiyi",
    "version": "0.1.1",
    "engines": {
        "vscode": "^1.60.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onView:yicode-funtree.embedded"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "yicode-funtree",
                    "title": "FunTree",
                    "icon": "resources/icons/funtree.svg"
                }
            ]
        },
        "views": {
            "yicode-funtree": [
                {
                    "id": "yicode-funtree.embedded",
                    "name": "FunTree"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile"
    }
