{
    "name": "@yicode-vscode/funtree",
    "displayName": "vscode 侧边栏函数列表插件",
    "description": "monorepo 多仓库 package.json 字段检测与补全工具",
    "publisher": "chensuiyi",
    "version": "0.1.2",
    "engines": {
        "vscode": "^1.60.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onView:yicode-funtree.embedded"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "yicode-funtree",
                    "title": "FunTree",
                    "icon": "resources/icons/funtree.svg"
                }
            ]
        },
        "views": {
            "yicode-funtree": [
                {
                    "id": "yicode-funtree.embedded",
                    "name": "FunTree"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile"
    },
    "private": false,
    "publishConfig": {
        "access": "public"
    },
    "type": "module",
    "author": "chensuiyi <bimostyle@qq.com>",
    "repository": {
        "type": "git",
        "url": "https://github.com/chenbimo/yicode.git"
    },
    "homepage": "https://yicode.tech"
}
